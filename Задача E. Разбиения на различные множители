def mnog(n, minmnog, spisok, sp):
    if n == 1:
        sp.append(spisok)
    else:
        for i in range(minmnog, n + 1):
            if n % i == 0:
                mnog(n // i, i + 1, spisok + [i], sp)
                if n == i:
                    return sp
 
 
n = int(input())
print(len(mnog(n, 2, [], [])))
